{
  "name": "jscodeshift-demo",
  "version": "0.1.0",
  "private": true,
  "description": "Demonstrate the use of jscodeshift",
  "license": "MIT",
  "repository": "marcusrbrown/jscodeshift-demo",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "bin": {
    "jscodeshift-demo": "./index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "jscodeshift",
    "transform"
  ],
  "dependencies": {
    "jscodeshift": "0.15.0"
  },
  "devDependencies": {
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "prettier": "2.8.8",
    "prettier-plugin-package": "1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "extends": "prettier"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 80,
    "singleQuote": true,
    "overrides": [
      {
        "files": "**/*.md",
        "options": {
          "parser": "markdown",
          "proseWrap": "always"
        }
      }
    ]
  }
}
